<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/mobile_profile"
    app:startDestination="@+id/navigation_input">

    <fragment
        android:id="@+id/navigation_input"
        android:name="com.example.sisuperjatim.InputFragment"
        android:label="@string/title_input"
        tools:layout="@layout/fragment_input" />

    <fragment
        android:id="@+id/navigation_maps"
        android:name="com.example.sisuperjatim.ui.maps.MapsFragment"
        android:label="@string/title_maps"
        tools:layout="@layout/fragment_maps" />

    <fragment
        android:id="@+id/navigation_profile"
        android:name="com.example.sisuperjatim.ui.profile.ProfileFragment"
        android:label="@string/title_profile"
        tools:layout="@layout/fragment_profile" />
</navigation>