                        android:textColor="#020202"
                        android:textSize="12dp"
                        android:layout_width="92dp"
                        android:layout_height="33dp"
                        android:autofillHints=""
                        tools:ignore="LabelFor" />
                </LinearLayout>
                <LinearLayout
                    android:layout_marginTop="10dp"
                    android:id="@+id/ll12"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:orientation="horizontal">
                    <TextView
                        android:id="@+id/tv_kcl"
                        android:layout_width="114dp"
                        android:layout_height="wrap_content"
                        android:text="kcl_kg_ha"
                        android:textColor="#020202"
                        android:textSize="14sp"
                        tools:ignore="DuplicateIds" />
                    <EditText
                        android:inputType="number"
                        android:id="@+id/kclMH"
                        android:gravity="center"
                        android:textColor="#020202"
                        android:textSize="12dp"
                        android:layout_width="92dp"
                        android:layout_height="33dp"
                        android:autofillHints=""
                        tools:ignore="LabelFor" />
                    <EditText
                        android:inputType="number"
                        android:id="@+id/kclMK1"
                        android:gravity="center"
                        android:textColor="#020202"
                        android:textSize="12dp"
                        android:layout_width="92dp"
                        android:layout_height="33dp"
                        android:autofillHints=""
                        tools:ignore="LabelFor" />
                    <EditText
                        android:inputType="number"
                        android:id="@+id/kclMK2"
                        android:gravity="center"
                        android:textColor="#020202"
                        android:textSize="12dp"
                        android:layout_width="92dp"
                        android:layout_height="33dp"
                        android:autofillHints=""
                        tools:ignore="LabelFor" />
                </LinearLayout>
                <LinearLayout
                    android:layout_marginTop="10dp"
                    android:id="@+id/ll13"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:orientation="horizontal">
                    <TextView
                        android:id="@+id/tv_phonska"
                        android:layout_width="114dp"
                        android:layout_height="wrap_content"
                        android:text="phonska_kg_ha"
                        android:textColor="#020202"
                        android:textSize="14sp"/>
                    <EditText
                        android:inputType="number"
                        android:id="@+id/phonskaMH"
                        android:gravity="center"
                        android:textColor="#020202"
                        android:textSize="12dp"
                        android:layout_width="92dp"
                        android:layout_height="33dp"
                        android:autofillHints=""
                        tools:ignore="LabelFor" />
                    <EditText
                        android:inputType="number"
                        android:id="@+id/phonskaMK1"
                        android:gravity="center"
                        android:textColor="#020202"
                        android:textSize="12dp"
                        android:layout_width="92dp"
                        android:layout_height="33dp"
                        android:autofillHints=""
                        tools:ignore="LabelFor" />
                    <EditText
                        android:inputType="number"
                        android:id="@+id/phonskaMK2"
                        android:gravity="center"
                        android:textColor="#020202"
                        android:textSize="12dp"
                        android:layout_width="92dp"
                        android:layout_height="33dp"
                        android:autofillHints=""
