<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    tools:context=".HubungiActivity">


    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="60dp"
        android:orientation="horizontal"
        android:layout_alignParentTop="true"
        android:background="@color/colorHijau">

        <TextView
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:text="Input Data Lapang"
            android:textColor="@color/colorPutih"
            android:textSize="20dp"
            android:gravity="center"
            android:layout_gravity="center"/>

    </LinearLayout>

    <ScrollView
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginStart="10dp"
        android:orientation="vertical">

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginTop="5dp"
            android:orientation="vertical"
            android:layout_gravity="center_horizontal">

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="40dp"
                android:layout_gravity="center_horizontal">


                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginStart="5dp"
                    android:text="Initial dan No Observasi"
                    android:textSize="14dp"
                    android:textStyle="bold"
                    />
                <EditText
                    android:id="@+id/edInitial"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginEnd="10dp"
                    android:layout_marginStart="5dp"/>

            </LinearLayout>


            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="40dp"
                android:layout_gravity="center_horizontal">

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginStart="5dp"
                    android:text="Tanggal"
                    android:textSize="14dp"
                    android:textStyle="bold"
                    />
                <EditText
                    android:id="@+id/edTangggal"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginEnd="10dp"
                    android:layout_marginStart="5dp"/>

            </LinearLayout>

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="30dp"
                android:layout_gravity="center_horizontal">

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginStart="5dp"
                    android:text="Provinsi"
                    android:textSize="14dp"
                    android:textStyle="bold"
                    />
                <Spinner
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginStart="5dp"
                    android:layout_marginEnd="10dp"/>

            </LinearLayout>

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="30dp"
                android:layout_gravity="center_horizontal">

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginStart="5dp"
                    android:text="KabKot"
                    android:textSize="14dp"
                    android:textStyle="bold"
                    />
                <Spinner
                    android:id="@+id/kabupaten"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginStart="10dp"
                    android:layout_marginEnd="10dp"/>

            </LinearLayout>


            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="30dp"
                android:layout_gravity="center_horizontal">

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginStart="5dp"
                    android:text="Kecamatan"
                    android:textSize="14dp"
                    android:textStyle="bold"
                    />
                <Spinner
                    android:id="@+id/kecamatan"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginStart="5dp"
                    android:layout_marginEnd="10dp"/>

            </LinearLayout>

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="30dp"
                android:layout_gravity="center_horizontal">

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginStart="5dp"
                    android:text="Desa"
                    android:textSize="14dp"
                    android:textStyle="bold"
                    />
                <Spinner
                    android:id="@+id/desa"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginStart="5dp"
                    android:layout_marginEnd="10dp"/>

            </LinearLayout>

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="30dp"
                android:layout_gravity="center_horizontal">

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginStart="5dp"
                    android:text="Koordinat"
                    android:textSize="14dp"
                    android:textStyle="bold"/>

                <com.google.android.gms.maps.MapView
                    android:id="@+id/mapinput"
                    android:layout_width="match_parent"
                    android:layout_height="20dp"
                    android:layout_marginStart="5dp"
                    android:layout_marginEnd="10dp"/>

            </LinearLayout>

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="100dp"
                android:layout_gravity="center_horizontal">

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginStart="5dp"
                    android:text="Foto"
                    android:textSize="14dp"
                    android:textStyle="bold"/>

                <ImageView
                    android:layout_width="100dp"
                    android:layout_height="100dp"
                    android:layout_marginStart="5dp"
                    android:layout_marginEnd="10dp"/>

            </LinearLayout>

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="30dp"
                android:layout_gravity="center_horizontal">

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginStart="5dp"
                    android:text="Jenis Lahan"
                    android:textSize="14dp"
                    android:textStyle="bold"
                    />
                <Spinner
                    android:id="@+id/jenis_lahan"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginStart="5dp"
                    android:layout_marginEnd="10dp"/>

            </LinearLayout>

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="30dp"
                android:layout_gravity="center_horizontal">

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginStart="5dp"
                    android:text="Bahan Induk"
                    android:textSize="14dp"
                    android:textStyle="bold"
                    />
                <Spinner
                    android:id="@+id/bahan_induk"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginStart="5dp"
                    android:layout_marginEnd="10dp"/>

            </LinearLayout>

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="30dp"
                android:layout_gravity="center_horizontal">

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginStart="5dp"
                    android:text="Penggunaan Lahan"
                    android:textSize="14dp"
                    android:textStyle="bold"
                    />
                <Spinner
                    android:id="@+id/penggunaan_lahan"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginStart="5dp"
                    android:layout_marginEnd="10dp"/>

            </LinearLayout>

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="40dp"
                android:layout_gravity="center_horizontal">

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginStart="5dp"
                    android:text="Lereng"
                    android:textSize="14dp"
                    android:textStyle="bold"
                    />
                <EditText
                    android:id="@+id/et_lereng"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginEnd="10dp"
                    android:layout_marginStart="5dp"/>

            </LinearLayout>

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="30dp"
                android:layout_gravity="center_horizontal">

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginStart="5dp"
                    android:text="Kondisi Lahan"
                    android:textSize="14dp"
                    android:textStyle="bold"
                    />
                <Spinner
                    android:id="@+id/kondisi_lahan"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginStart="5dp"
                    android:layout_marginEnd="10dp"/>

            </LinearLayout>


            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="30dp"
                android:layout_gravity="center_horizontal">

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="Drainese"
                    android:textStyle="bold"
                    android:layout_marginStart="5dp"
                    android:textSize="14dp"/>
                <Spinner
                    android:id="@+id/drainase"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginStart="5dp"
                    android:layout_marginEnd="10dp"/>

            </LinearLayout>

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="30dp"
                android:layout_gravity="center_horizontal">

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="Reaksi Tanah"
                    android:textStyle="bold"
                    android:layout_marginStart="5dp"
                    android:textSize="14dp"/>
                <Spinner
                    android:id="@+id/reaksi_tanah"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginStart="5dp"
                    android:layout_marginEnd="10dp"/>

            </LinearLayout>

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="30dp"
                android:layout_gravity="center_horizontal">

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="Tanaman Utama"
                    android:textStyle="bold"
                    android:layout_marginStart="5dp"
                    android:textSize="14dp"/>
                <Spinner
                    android:id="@+id/tanaman_utama"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginStart="5dp"
                    android:layout_marginEnd="10dp"/>

            </LinearLayout>

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="30dp"
                android:layout_gravity="center_horizontal">

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="Tekstur"
                    android:textStyle="bold"
                    android:layout_marginStart="5dp"
                    android:textSize="14dp"/>
                <Spinner
                    android:id="@+id/tekstur"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginStart="5dp"
                    android:layout_marginEnd="10dp"/>

            </LinearLayout>

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="40dp"
                android:layout_gravity="center_horizontal">

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginStart="5dp"
                    android:text="Kedalaman Olah"
                    android:textSize="14dp"
                    android:textStyle="bold"
                    />
                <EditText
                    android:id="@+id/et_kedalaman"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginEnd="10dp"
                    android:layout_marginStart="5dp"/>

            </LinearLayout>


            <LinearLayout
                android:id="@+id/hidelayout"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:orientation="vertical">

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:orientation="horizontal">

                    <TextView
                        android:id="@+id/tv_tanam"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="Musim Tanam"
                        android:textColor="#155C1D"
                        android:textSize="14dp"
                        android:textStyle="bold" />
                    <TextView
                        android:gravity="center"
                        android:textStyle="bold"
                        android:textColor="#155C1D"
                        android:textSize="14sp"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginStart="50dp"
                        android:layout_marginEnd="10dp"
                        android:text="MH"
                        tools:ignore="HardcodedText" />
                    <TextView
                        android:gravity="center"
                        android:textStyle="bold"
                        android:textColor="#155C1D"
                        android:textSize="14sp"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginStart="50dp"
                        android:layout_marginEnd="10dp"
                        android:text="MK-1"
                        tools:ignore="HardcodedText" />
                    <TextView
                        android:gravity="center"
                        android:textStyle="bold"
                        android:textColor="#155C1D"
                        android:textSize="14sp"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginStart="50dp"
                        android:layout_marginEnd="10dp"
                        android:text="MK-2"
                        tools:ignore="HardcodedText" />

                </LinearLayout>


                <LinearLayout
                    android:layout_marginTop="10dp"
                    android:id="@+id/ll7"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:orientation="horizontal">
                    <TextView
                        android:id="@+id/tv_polatanam"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="- Pola Tanam"
                        android:textColor="#020202"
                        android:textSize="14sp"
                        tools:ignore="HardcodedText" />
                    <Spinner
                        android:id="@+id/polaMH"
                        android:gravity="center"
                        android:textColor="#020202"
                        android:textSize="13sp"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"/>
                    <Spinner
                        android:id="@+id/polaMK1"
                        android:gravity="center"
                        android:textColor="#020202"
                        android:textSize="13sp"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"/>
                    <Spinner
                        android:id="@+id/polaMK2"
                        android:gravity="center"
                        android:textColor="#020202"
                        android:textSize="13sp"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"/>
                </LinearLayout>

                <LinearLayout
                    android:layout_marginTop="10dp"
                    android:id="@+id/ll8"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:orientation="horizontal">
                    <TextView
                        android:id="@+id/tv_varietas"
                        android:layout_width="114dp"
                        android:layout_height="wrap_content"
                        android:text="- Varietas"
                        android:textColor="#020202"
                        android:textSize="14sp"
                        tools:ignore="HardcodedText" />
                    <Spinner
                        android:id="@+id/varietasMH"
                        android:gravity="center"
                        android:textColor="#020202"
                        android:textSize="13sp"
                        android:layout_width="92dp"
                        android:layout_height="25dp"/>
                    <Spinner
                        android:id="@+id/varietasMK1"
                        android:gravity="center"
                        android:textColor="#020202"
                        android:textSize="13sp"
                        android:layout_width="92dp"
                        android:layout_height="25dp"/>
                    <Spinner
                        android:id="@+id/varietasMK2"
                        android:gravity="center"
                        android:textColor="#020202"
                        android:textSize="13sp"
                        android:layout_width="92dp"
                        android:layout_height="25dp"/>
                </LinearLayout>
                <LinearLayout
                    android:layout_marginTop="10dp"
                    android:id="@+id/ll9"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:orientation="horizontal">
                    <TextView
                        android:id="@+id/tv_provitas"
                        android:layout_width="114dp"
                        android:layout_height="wrap_content"
                        android:text="- Provitas"
                        android:textColor="#020202"
                        android:textSize="14sp"
                        tools:ignore="HardcodedText" />

                    <EditText
                        android:id="@+id/provitasMH"
                        android:layout_width="92dp"
                        android:layout_height="33dp"
                        android:autofillHints=""
                        android:gravity="center"
                        android:inputType="number"
                        android:textColor="#020202"
                        android:textSize="12dp"
                        tools:ignore="LabelFor,TextFields" />

                    <EditText
                        android:id="@+id/provitasMK1"
                        android:layout_width="92dp"
                        android:layout_height="33dp"
                        android:autofillHints=""
                        android:gravity="center"
                        android:inputType="number"
                        android:textColor="#020202"
                        android:textSize="12dp"
                        tools:ignore="LabelFor" />
                    <EditText
                        android:id="@+id/provitasMK2"
                        android:gravity="center"
                        android:textColor="#020202"
                        android:textSize="12dp"
                        android:layout_width="92dp"
                        android:layout_height="33dp"
                        android:inputType="number"
                        android:autofillHints=""
                        tools:ignore="LabelFor" />
                </LinearLayout>
                <TextView
                    android:layout_marginTop="5dp"
                    android:textColor="#155C1D"
                    android:text="Jenis Pupuk"
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:textSize="16dp"
                    android:textStyle="bold"/>
                <LinearLayout
                    android:layout_marginTop="5dp"
                    android:id="@+id/ll10"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:orientation="horizontal">
                    <TextView
                        android:id="@+id/tv_organik"
                        android:layout_width="114dp"
                        android:layout_height="wrap_content"
                        android:text="organik_kg_ha"
                        android:textColor="#020202"
                        android:textSize="14sp"/>
                    <EditText
                        android:inputType="number"
                        android:id="@+id/organikMH"
                        android:gravity="center"
                        android:textColor="#020202"
                        android:layout_width="92dp"
                        android:layout_height="33dp"
                        android:textSize="12dp"
                        android:autofillHints=""
                        tools:ignore="LabelFor" />
                    <EditText
                        android:inputType="number"
                        android:id="@+id/organikMK1"
                        android:gravity="center"
                        android:textColor="#020202"
                        android:textSize="12dp"
                        android:layout_width="92dp"
                        android:layout_height="33dp"
                        android:autofillHints=""
                        tools:ignore="LabelFor" />
                    <EditText
                        android:inputType="number"
                        android:id="@+id/organikMK2"
                        android:gravity="center"
                        android:textColor="#020202"
                        android:textSize="12dp"
                        android:layout_width="92dp"
                        android:layout_height="33dp"
                        android:autofillHints=""
                        tools:ignore="LabelFor" />
                </LinearLayout>
                <LinearLayout
                    android:layout_marginTop="10dp"
                    android:id="@+id/ll11"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:orientation="horizontal">
                    <TextView
                        android:id="@+id/tv_urea"
                        android:layout_width="114dp"
                        android:layout_height="wrap_content"
                        android:text="urea_kg_ha"
                        android:textColor="#020202"
                        android:textSize="14sp"
                        tools:ignore="TooManyViews" />
                    <EditText
                        android:inputType="number"
                        android:id="@+id/ureaMH"
                        android:gravity="center"
                        android:textColor="#020202"
                        android:textSize="12dp"
                        android:layout_width="92dp"
                        android:layout_height="33dp"
                        android:autofillHints=""
                        tools:ignore="LabelFor" />
                    <EditText
                        android:inputType="number"
                        android:id="@+id/ureaMK1"
                        android:gravity="center"
                        android:textColor="#020202"
                        android:textSize="12dp"
                        android:layout_width="92dp"
                        android:layout_height="33dp"
                        android:autofillHints=""
